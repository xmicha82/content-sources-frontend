    "prop-types": "15.8.0",
    "ts-jest": "^29.1.1",
    "ts-patch": "^3.0.2",
    "typescript": "^5.2.2",
    "webpack-bundle-analyzer": "^4.9.1"
  },
  "insights": {
    "appname": "content-sources",
    "buildrepo": "git@github.com:RedHatInsights/content-sources-frontend-build.git"
